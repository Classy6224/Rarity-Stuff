local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wall%20v3')))()

local w = library:CreateWindow("A")
local b = w:CreateFolder("B")

-- Money Roll Toggle
local toggleMoneyRoll = false

b:Toggle("Toggle Money Roll", function(bool)
    toggleMoneyRoll = bool
    print("Toggle Money Roll:", toggleMoneyRoll)
    
    while toggleMoneyRoll do
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Open_S"):FireServer()
        wait(1) -- You may adjust the wait time as needed
    end
end)

-- Rebirth Toggle
local toggleRebirth = false

b:Toggle("Toggle Rebirth", function(bool)
    toggleRebirth = bool
    print("Toggle Rebirth:", toggleRebirth)
    
    while toggleRebirth do
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Rebirth"):WaitForChild("PurchaseRebirth"):FireServer()
        wait(1) -- You may adjust the wait time as needed
    end
end)

-- Rest of your UI elements (buttons, sliders, etc.)

b:DestroyGui()
